public class CalcHoraEstacionamento {
    
    private double valorHora= 5.5;
    private double horaInicial=13.00;
    private double horaFinal=14.00;
    private double horaTotal;
    private double valorFinal;

    public static void main(String[] args) {
            
    }

     public static void main(String[] args) {
        
         horaTotal = horaFinal-horaInicial;
         CALCULADORA.Piso(horaTotal);
         valorFinal = horaTotal*valorHora;
         return(valorFinal);
     }

     System.out.println("O valor a ser pago Ã© de: "+valorFinal);

}
